{
  "name": "jewelry.shop",
  "description": "Jewelry Social Commerce",
  "version": "0.0.1",
  "license": "DZR",
  "private": true,
  "scripts": {
    "dev": "run-p dev:pcss dev:run",
    "dev:pcss": "postcss src/styles/global.pcss -o static/global.css -w",
    "dev:run": "sapper dev",
    "css:pcss": "postcss src/styles/global.pcss -o static/global.css",
    "build": "run-s build:pcss build:sapper",
    "build:pcss": "cross-env NODE_ENV=production postcss src/styles/global.pcss -o static/global.css",
    "build:sapper": "sapper build --legacy",
    "build:export": "sapper export --legacy",
    "real": "run-s build:pcss real:sapper",
    "real:sapper": "cross-env NODE_ENV=production sapper build --legacy",
    "real:export": "cross-env NODE_ENV=production sapper export --legacy",
    "export": "run-s build:pcss build:export",
    "start": "node __sapper__/build",
    "serve": "serve ___sapper__/export",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "polka": "next",
    "sirv": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-url": "^5.0.1",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@tailwindcss/typography": "^0.2.0",
    "autoprefixer": "^10.0.1",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.4",
    "postcss-cli": "^8.2.0",
    "postcss-import": "^13.0.0",
    "postcss-input-range": "^4.0.0",
    "postcss-load-config": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.0",
    "rollup": "^2.32.1",
    "rollup-plugin-svelte": "^6.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.10",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "svelte": "^3.29.4",
    "svelte-fa": "^2.1.1",
    "svelte-preprocess": "^4.5.2",
    "tailwindcss": "^1.9.6"
  }
}
